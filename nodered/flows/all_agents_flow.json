    {
        "id": "http_request_agent",
        "type": "http request",
        "z": "all_agents_tab",
        "name": "Call Agent API",
        "method": "POST",
        "ret": "obj",
        "paytoqs": "ignore",
        "url": "",
        "tls": "",
        "persist": false,
        "proxy": "",
        "authType": "",
        "sendsPayload": true,
        "x": 440,
